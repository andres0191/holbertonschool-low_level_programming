#Modify Makefile with other requirements

CC = gcc
SRC = main.c holberton.c
OBJ = $(SRC:%.c=%.o)
NAME = holberton
RM = rm -rf
CFLAGS = -Wall -Werror -Wextra -pedantic

all: $(OBJ)
	$(CC) $(CLFAGS) $(OBJ) -o $(NAME)

clean:
	$(RM) *~ $(NAME)

oclean:
	$(RM) $(OBJ)

fclean: clean oclean

re: oclean all
